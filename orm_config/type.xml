<table name="type" namespace="blog.data" desc="博客分类表">
    <columns>
        <column name="id" type="int64" auto_increment="true" desc="自增id"/>
        <column name="type_name" type="string" length="50" desc="分类名称"/>
        <column name="create_time" type="datetime" desc="创建时间"/>
        <column name="update_time" type="datetime" update="current_timestamp" desc="更新时间"/>
    </columns>
    <indexs>
        <index name="pk" cols="id" type="pk"/>
        <index name="type_name" cols="type_name" type="uniq"/>
    </indexs>
</table>
