<table name="tag" namespace="blog.data" desc="文章标签">
    <columns>
        <column name="id" type="int64" auto_increment="true" desc="标签id"/>
        <column name="tag_name" type="string" length="50" desc="标签名称"/>
        <column name="create_time" type="datetime" desc="创建时间"/>
        <column name="update_time" type="datetime" desc="更新时间"/>
    </columns>
    <indexs>
        <index name="pk" cols="id" type="pk"/>
        <index name="tag_name" cols="tag_name" type="uniq"/>
    </indexs>
</table>
